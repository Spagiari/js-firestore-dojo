{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "disableSort": false,
    "collapse": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/braintree-jsdoc-template"
  }
}
